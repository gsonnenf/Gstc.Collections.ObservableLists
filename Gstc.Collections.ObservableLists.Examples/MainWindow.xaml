<Window x:Class="Gstc.Collections.ObservableLists.Examples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:observableListSync="clr-namespace:Gstc.Collections.ObservableLists.Examples.ObservableListSync"
        xmlns:observableList="clr-namespace:Gstc.Collections.ObservableLists.Examples.ObservableList"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl TabStripPlacement="Bottom">
            <TabItem Header="Observable List Control">
                <observableList:ObservableListControl></observableList:ObservableListControl>
            </TabItem>
            <TabItem Header="Observable List Sync">
                <observableListSync:ObservableListSyncControl x:Name="ObvListSync"></observableListSync:ObservableListSyncControl>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
